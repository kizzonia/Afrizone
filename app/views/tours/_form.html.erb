<%= simple_form_for(@tour) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :timimg %>
    <%= f.input :date %>
    <%= f.input :place %>
    <%= f.input :artist %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
